{
    "liq-schema": {
      "prefix": "liq-schema",
      "body": [
        "{% schema %}",
        "{",
        "    \"name\": {",
        "        \"en\": \"Carousel\"",
        "    },",
        "    \"settings\": [",
        "",
        "    ],",
        "    \"presets\": [",
        "    {",
        "        \"name\": \"Carousel\",",
        "        \"category\": \"Carousel\"",
        "    }",
        "  ]",
        "}",
        "{% endschema %}"
      ],
      "description": "liq-schema"
    },
    "liq-text": {
      "prefix": "liq-text",
      "body": [
        "{",
        "     \"type\": \"text\",",
        "     \"id\": \"${1:unique_id}\",",
        "     \"label\": \"${2:Label}\",",
        "     \"info\": \"${3:Info}\"",
        "},"
      ],
      "description": "liquid content schema text"
    },
    "liq-checkbox": {
      "prefix": "liq-checkbox",
      "body": [
        "{",
        "  \"type\": \"checkbox\",",
        "  \"id\": \"${1:example}\",",
        "  \"label\": \"${2:example}\",",
        "  \"default\": true",
        "}"
      ],
      "description": "liq-checkbox"
    },
    "liq-radio": {
      "prefix": "liq-radio",
      "body": [
        "{",
        "  \"type\": \"radio\",",
        "  \"id\": \"${1:example}\",",
        "  \"label\": \"${2:example}\",",
        "  \"options\": [",
        "    {",
        "      \"value\": \"${3:example}\",",
        "      \"label\": \"${4:example}\"",
        "    },",
        "    {",
        "      \"value\": \"${5:example}\",",
        "      \"label\": \"${6:example}\"",
        "    }",
        "  ],",
        "  \"default\": \"${3:example}\"",
        "}"
      ],
      "description": "liq-radio"
    },
    "liq-range": {
      "prefix": "liq-range",
      "body": [
        "{",
        "  \"type\": \"range\",",
        "  \"id\": \"${1:example}\",",
        "  \"label\": \"${2:example}\",",
        "  \"min\": ${3:example},",
        "  \"max\": ${4:example},",
        "  \"step\": ${5:example},",
        "  \"default\": ${6:example},",
        "  \"info: \"${7:example}\"",
        "}"
      ],
      "description": "liq-range"
    },
    "liq-select": {
      "prefix": "liq-select",
      "body": [
        "{",
        "  \"type\": \"select\",",
        "  \"id\": \"${1:example}\",",
        "  \"label\": \"${2:example}\",",
        "  \"options\": [",
        "    {",
        "      \"value\": \"${3:example}\",",
        "      \"label\": \"${4:example}\"",
        "    },",
        "    {",
        "      \"value\": \"${5:example}\",",
        "      \"label\": \"${6:example}\"",
        "    }",
        "  ],",
        "  \"default\": \"${3:example}\"",
        "}"
      ],
      "description": "liq-select"
    },
    "liq-header": {
      "prefix": "liq-header",
      "body": [
        "{",
        "  \"type\": \"header\",",
        "  \"content\": \"${1:example}\"",
        "}"
      ],
      "description": "liq-header"
    },
    "liq-html": {
      "prefix": "liq-html",
      "body": [
        "{",
        "   \"type\": \"html\",",
        "   \"id\": \"${1:example}\",",
        "   \"label\": \"${2:Custom HTML}\",",
        "   \"default\": \"${3:<div><p>Some HTML content</p></div>}\"",
        "}"
      ],
      "description": "liq-html"
    },
    "liq-video-url": {
      "prefix": "liq-video-url",
      "body": [
        "{",
        "  \"id\": \"${1:video_url}\",",
        "  \"type\": \"video_url\",",
        "  \"label\": \"${2:Video URL}\",",
        "  \"accept\": [\"youtube\", \"vimeo\"],",
        "  \"default\": \"https://www.youtube.com/watch?v=_9VUPq3SxOc\",",
        "  \"info\": \"${3:Text}\",",
        "  \"placeholder\": \"${4:Text}\"",
        "}"
      ],
      "description": "liq-video-url"
    },
    "liq-page": {
      "prefix": "liq-page",
